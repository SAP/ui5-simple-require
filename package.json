    }
  ],
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0"
  },
  "dependencies": {
    "deepmerge": "^2.0.1",
    "lodash.set": "^4.3.2",
    "lodash.topairs": "^4.3.0"
  }
}
