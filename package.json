{
  "name": "ui5-simple-require",
  "version": "1.0.0",
  "description": "Import UI5 modules into NodeJS applications, allowing isolation of UI5 components and injection of dependencies to create an isolated test environment",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "./node_modules/mocha/bin/mocha test/*Test.js",
    "test:debug": "./node_modules/mocha/bin/mocha --inspect-brk test/*Test.js",
    "testwatcher": "node node_modules/mocha/bin/mocha test/*Test.js --colors --recursive --watch"
  },
  "keywords": [
    "npm",
    "mocha",
    "ui5"
  ],
